  "dependencies": {
    "ant-design-vue": "^4.0.0-rc.6",
    "rough-notation": "^0.5.1",
    "vitepress-plugin-image-viewer": "^1.1.6"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
