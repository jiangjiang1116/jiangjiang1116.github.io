{
  "version": "1.0.0",
  "devDependencies": {
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "vitepress": "^1.6.3"
  },
  "scripts": {
    "docs:dev": "npm run gen-sidebar && vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "gen-sidebar": "node scripts/gen-sidebar.mjs",
